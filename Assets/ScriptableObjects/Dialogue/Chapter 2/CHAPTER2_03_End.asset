  m_Script: {fileID: 11500000, guid: 46b93f98c47f03b48944e4c67253c1b9, type: 3}
  m_Name: CHAPTER2_03_End
  m_EditorClassIdentifier: 
  eventToTriggerOnEnd: EndChapter2
  DialogueSteps:
  - CharacterName: 
    Text: "You\u2019ve completed all required care actions again."
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: 
    Text: Stats are well within range.
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: 
    Text: You sit back in your chair as the task completion ping sounds.
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: KEY
    Text: Subject 121 remains in satisfactory condition.
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: 
    Text: You look over.
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: 
    Text: "It\u2019s at the glass again. \r"
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: 
    Text: "It hasn\u2019t moved all day, and now it\u2019s closer than ever\u2026
      too close\u2026\r"
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: 
    Text: That eye, open, unblinking, reflects your own screen back at you.
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: 
    Text: "It\u2019s no longer hidden. It wants to be seen."
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: 
    Text: "For a moment, its fur shifts strangely\u2026"
    DialogueOptions: []
    eventToTrigger: PanelNone
  - CharacterName: 
    Text: Almost like a second layer is twitching underneath.
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: 
    Text: 'Then: stillness.'
    DialogueOptions: []
    eventToTrigger: PanelDefault
  - CharacterName: KEY
    Text: Behaviour patterns are consolidating. You are making progress, Caretaker.
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: 
    Text: The light in the room dims.
    DialogueOptions: []
    eventToTrigger: PanelBlack
  - CharacterName: 
    Text: 'Your screen flickers. '
    DialogueOptions: []
    eventToTrigger: PanelDefault
  - CharacterName: 
    Text: For half a second, you see a single frame glitch, the creature distorted.
    DialogueOptions: []
    eventToTrigger: TriggerPetCareReboot
  - CharacterName: 
    Text: ...unreadable.
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: 
    Text: You think you see the eye open wider than it should be..
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: KEY
    Text: ...That should not have happened.
    DialogueOptions: []
    eventToTrigger: PanelBlack
  - CharacterName: KEY
    Text: Initiating backup protocol.
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: 
    Text: ...
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: 
    Text: Silence.
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: 
    Text: 'Then the screen returns to normal. '
    DialogueOptions: []
    eventToTrigger: PanelDefault
  - CharacterName: 
    Text: "Subject 121 is back in its corner. The eye is\u2026 closed."
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: KEY
    Text: Phase adaptation, will begin tomorrow.
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: KEY
    Text: You are relieved of your duties.
    DialogueOptions: []
    eventToTrigger: 
  - CharacterName: KEY
    Text: Have a good night's rest, Caretaker {PLAYER_NAME}.
    DialogueOptions: []
    eventToTrigger: PanelBlack
